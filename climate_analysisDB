--hawaii_measurements.csv
DROP TABLE IF EXISTS hawaii_measurements;

CREATE TABLE hawaii_measurements (
	station VARCHAR DEFAULT NULL,
	date DATE DEFAULT NULL,
	prcp decimal DEFAULT NULL,
	tobs INT DEFAULT NULL
);
